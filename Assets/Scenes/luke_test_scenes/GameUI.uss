#BatteryFace.hidden {
    display: none;
}

#BatteryBGCont.hidden {
    display: none;
}

#TimerContainer {
    translate: 0 -214px;
    scale: 0.75;
    opacity: 0;
    transition-property: opacity, scale, translate;
    transition-duration: 2s;
}

#TimerContainer.show {
    scale: 1;
    opacity: 1;
    translate: 0 0;
}

#TimerContainer.hidden {
    display: none;
}

.ability-tip {
    scale: 0.75;
    opacity: 0;
    transition-property: opacity, scale;
    transition-duration: 0.25s;
}

.ability-tip.show {
    scale: 1;
    opacity: 1;
}

.ability-tip.hidden {
    display: none;
}

.modal {
    scale: 0.75;
    opacity: 0;
    transition-property: opacity, scale;
    transition-duration: 0.25s;
}

.modal.show {
    scale: 1;
    opacity: 1;
}

.modal.hidden {
    display: none;
}

#PauseBG {
    scale: 0.75;
    opacity: 0;
    transition-property: opacity, scale;
    transition-duration: 0.25s;
}

#PauseBG.show {
    scale: 1;
    opacity: 1;
}

#PauseBG.hidden {
    display: none;
}

#VictoryBG {
    scale: 0.75;
    opacity: 0;
    transition-property: opacity, scale;
    transition-duration: 0.25s;
}

#VictoryBG.show {
    scale: 1;
