}

.lower-left {
    transition-property: translate, scale;
    transition-duration: 0.25s;
}

.lower-left.hidden {
    translate: -700px 0;
    scale: 0;
}

#CutsceneText {
    transition-property: translate, scale;
    transition-duration: 1s;
}
